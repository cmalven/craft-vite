{
    "repositories": [],
    "require": {
        "craftcms/cms": "^4.0.0",
        "nystudio107/craft-autocomplete": "^1.10",
        "nystudio107/craft-emptycoalesce": "^4.0.0-beta.1",
        "vlucas/phpdotenv": "^5.4.0"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        }
    }
}
